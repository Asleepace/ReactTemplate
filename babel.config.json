{
    "plugins": [
        ["babel-plugin-root-import", {
            "paths": [{
                "rootPathSuffix": "./",
                "rootPathPrefix": "~/"
            },{
                "rootPathSuffix": "@/",
                "rootPathPrefix": "./src/assets/css/"
            },{
                "rootPathSuffix": "#/",
                "rootPathPrefix": "./src/app/assets"
            }]
        }]
    ]
}